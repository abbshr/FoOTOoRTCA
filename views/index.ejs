<!doctype html>
<html>
<head>
<meta content="text/html; charset=utf-8">
<title><%= title %>-RTCA</title>
<link type="text/css" rel="stylesheet" href="stylesheets/common.css">
<link type="text/css" rel="stylesheet" href="stylesheets/chat.css">
</head>
<body>
  <div id="contain">
    <div id="content">
      <div id="content_show">
        <div id="contents"></div>
      </div>
      <div id="content_saying">
        <div id="toolbar"></div>
        <div id="input_content" contenteditable="true"></div>
        <div id="sayingto">Hi~, <span id="from"></span>, 你正在对 <span id="to"></span>说</div>
        <div id="say">发送</div>
      </div>
    </div>
    <div id="users_online">
      <div id="usersbar">
        <div id="user_label">在线用户</div>
        <div id="users_list">
          <ul id="list"></ul>
        </div>
      </div>
    </div>
  </div>
<script src="/socket.io/socket.io.js"></script>
<script src="javascripts/chat.js"></script>
</body>
</html>
