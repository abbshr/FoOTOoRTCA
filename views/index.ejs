  		<button id="say" class="pull-right btn btn-danger">发送消息</button>
  		<br>
  	</div>
  </div>
  </div>
  <br>
  <br>
  <div class="well">
  	<center>RTCA ©2013</center> 
	</div>
<script src="/socket.io/socket.io.js"></script>
<script src="javascripts/chat.js"></script>
