<div class="">
  <% rooms.forEach(function (room, index) { %>
    <div class="">
      <h5><%= room.title %></h5>
      <label>创建时间:<%= room.settime %></label>
      <label><%= room.isPrivate ? '私秘Topic' : '公共Topic' %></label>
      <label>成员: <%= room.member.length %></label>
      <a href="/room/get/<%= encodeURIComponent(room._id) %>">加入Topic</a>
    <% if ((room.isPrivate && (room.member.indexOf(locals.user.name) != -1)) || !room.isPrivate) { %>
      <a href="/room/history?roomId=<%= escape(room._id) %>">历史记录</a>
    <% } %>
    </div>
  <% }) %>
</div>